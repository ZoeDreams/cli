{
    "name": "builder-js-package",
    "version": "0.0.0",
    "description": "A generic build setup for JavaScript packages.",
    "homepage": "https://github.com/trusktr/builder-js-packages",
    "repository": {
        "type": "git",
        "url": "git@github.com:trusktr/builder-js-packages.git"
    },
    "bugs": {
        "url": "https://github.com/trusktr/builder-js-packages/issues"
    },
    "scripts": {
        "show:name": "node ./node_modules/builder-js-package/scripts/name.js",
        "build:clean": "git clean -xfd -e node_modules",
        "build:prod": "builder run show:name && rm -rf package-lock.json && builder run build:es && builder run build:global",
        "build:dev": "builder run show:name && builder run build:dev:es",
        "build:dev:es": "builder run build:es:watch & P1=$!; builder run build:global:watch & P2=$!; wait $P1 $P2",
        "gulp": "gulp --cwd . --gulpfile ./node_modules/builder-js-package/config/gulpfile.js",
        "webpack": "webpack --config ./node_modules/builder-js-package/config/webpack.config.js",
        "build:es": "builder run gulp -- build-es",
        "build:global": "builder run webpack",
        "build:es:watch": "builder run gulp -- watch-es",
        "build:global:watch": "builder run build:global -- --progress --watch",
        "release:pre": "./node_modules/builder-js-package/scripts/release:pre.sh",
        "release:patch": "builder run release:pre && npm version patch -m 'v%s'",
        "release:minor": "builder run release:pre && npm version minor -m 'v%s'",
        "release:major": "builder run release:pre && npm version major -m 'v%s'",
        "npm:test": "./node_modules/builder-js-package/scripts/run-karma-tests.sh",
        "npm:test-debug": "KARMA_DEBUG=true ./node_modules/builder-js-package/scripts/run-karma-tests.sh",
        "npm:version": "./node_modules/builder-js-package/scripts/version.sh",
        "npm:postversion": "./node_modules/builder-js-package/scripts/postversion.sh"
    },
    "dependencies": {
        "@babel/cli": "7.0.0-beta.46",
        "@babel/core": "7.0.0-beta.46",
        "@babel/plugin-proposal-export-namespace-from": "7.0.0-beta.46",
        "@babel/plugin-proposal-object-rest-spread": "7.0.0-beta.46",
        "@babel/preset-env": "7.0.0-beta.46",
        "@babel/register": "7.0.0-beta.46",
        "@babel/runtime": "7.0.0-beta.46",
        "@types/jasmine": "^3.3.13",
        "@types/karma": "^3.0.3",
        "babel-loader": "8.0.0-beta.2",
        "babel-plugin-add-module-exports": "0.2.1",
        "electron": "^2.0.0",
        "figlet": "^1.2.0",
        "globby": "^8.0.1",
        "gulp": "4.0.0",
        "gulp-babel": "8.0.0-beta.2",
        "gulp-cached": "1.1.1",
        "gulp-typescript": "^5.0.1",
        "jasmine-core": "^3.1.0",
        "karma": "infamous/karma#disable-ts-node",
        "karma-electron": "^6.0.0",
        "karma-jasmine": "^1.1.1",
        "karma-sourcemap-loader": "^0.3.7",
        "karma-spec-reporter": "0.0.32",
        "karma-stacktrace": "^1.0.3",
        "lodash.merge": "^4.6.1",
        "merge-stream": "^2.0.0",
        "mkdirp": "^0.5.1",
        "regexr": "^1.4.4",
        "rimraf": "^2.6.3",
        "ts-loader": "^6.0.4",
        "ts-node": "^8.3.0",
        "typescript": "^3.5.2",
        "webpack": "4.27.1",
        "webpack-cli": "3.2.3",
        "webpack-merge": "4.1.2",
        "xvfb-maybe": "^0.2.1"
    },
    "keywords": [
        "build",
        "builder",
        "archetype",
        "build-archetype",
        "babel",
        "webpack",
        "web-development",
        "webdev",
        "karma",
        "electron"
    ]
}
